{
  "name": "@ebizbase/vanila-chrome-extension",
  "version": "1.0.0",
  "description": "Chrome extension with angular boilerplate",
  "license": "MIT",
  "engines": {
    "node": "^18.19.1 || ^20.11.1",
    "yarn": ">=1.22.22 <2"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "prepare": "husky",
    "start:dev": "nx run-many --target=build --projects=tag:components --configuration development --watch --parallel 6",
    "start:prod": "nx run-many --target=build --projects=tag:components --configuration production --watch --parallel 6",
    "build:dev": "nx run-many --target=build --projects=tag:components --configuration development --parallel 6",
    "build:prod": "nx run-many --target=build --projects=tag:components --configuration production --parallel 6",
    "test:unit": "jest . --coverage",
    "test:e2e": "nx run e2e:test",
    "test:ci": "jest --ci --coverage && nx run e2e:test"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less,scss,html,json,graphql,md,yaml,yml,vue}": [
      "npx prettier --write"
    ],
    "*.package.json": [
      "npx sort-package-json"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.0.0",
    "@angular/common": "~19.0.0",
    "@angular/compiler": "~19.0.0",
    "@angular/core": "~19.0.0",
    "@angular/forms": "~19.0.0",
    "@angular/platform-browser": "~19.0.0",
    "@angular/platform-browser-dynamic": "~19.0.0",
    "@angular/router": "~19.0.0",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@action-validator/cli": "^0.6.0",
    "@action-validator/core": "^0.6.0",
    "@angular-devkit/build-angular": "~19.0.0",
    "@angular-devkit/core": "~19.0.0",
    "@angular-devkit/schematics": "~19.0.0",
    "@angular/cli": "~19.0.0",
    "@angular/compiler-cli": "~19.0.0",
    "@angular/language-service": "~19.0.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "20.3.0",
    "@nx/devkit": "20.3.0",
    "@nx/eslint": "20.3.0",
    "@nx/eslint-plugin": "20.3.0",
    "@nx/jest": "20.3.0",
    "@nx/js": "20.3.0",
    "@nx/playwright": "20.3.0",
    "@nx/web": "20.3.0",
    "@nx/workspace": "20.3.0",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "~19.0.0",
    "@swc-node/register": "~1.10.9",
    "@swc/core": "~1.10.3",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "^0.2.37",
    "@swc/types": "^0.1.17",
    "@types/chrome": "^0.0.287",
    "@types/cpx": "^1.5.5",
    "@types/jest": "^29.5.12",
    "@types/node": "18.19.68",
    "@typescript-eslint/parser": "^8.18.2",
    "@typescript-eslint/utils": "^8.13.0",
    "angular-eslint": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "cpx": "^1.5.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^2.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.4.0",
    "lint-staged": "^15.2.11",
    "nx": "20.3.0",
    "postcss": "^8.4.49",
    "prettier": "^2.6.2",
    "sort-package-json": "^2.12.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.2",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "webpack": "^5.97.1"
  }
}
